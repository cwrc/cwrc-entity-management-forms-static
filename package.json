{
  "name": "cwrc-entity-forms",
  "version": "0.2.0",
  "private": true,
  "homepage": "/",
  "scripts": {
    "start": "npm run use-local react-scripts start",
    "build-dev02": "npm run use-dev02 react-scripts build",
    "build-prod": "npm run use-prod react-scripts build",
    "postbuild": "react-snapshot",
    "test": "react-scripts test --env=jsdom",
    "use-prod": "env-cmd -e production",
    "use-dev02": "env-cmd -e development",
    "use-local": "env-cmd -e local",
    "eject": "react-scripts eject",
    "build-semantic": "cd src/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && npm run build-semantic && gulp watch",
    "build-dependency-graph": "depcruise --include-only \"^src\" --output-type dot src | dot -Gsplines=ortho -T svg | depcruise-wrap-stream-in-html > ./dependency-graph/dependency-graph.html",
    "cm": "git-cz"
  },
  "dependencies": {
    "broadcast-channel": "^3.1.0",
    "cwrc-public-entity-dialogs": "^3.0.1",
    "cwrc-tei-entities-lookup": "^2.0.0",
    "dbpedia-entity-lookup": "^1.0.0",
    "geonames-entity-lookup": "^2.0.0",
    "getty-entity-lookup": "^2.0.0",
    "history": "^4.7.2",
    "iso-639-2": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^3.4.1",
    "react-widgets": "^4.3.2",
    "react-widgets-moment": "^4.0.15",
    "redux": "^4.0.0",
    "redux-form": "^7.4.2",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "styled-components": "^5.0.1",
    "type-to-reducer": "^1.1.0",
    "viaf-entity-lookup": "^2.0.0",
    "wikidata-entity-lookup": "^2.0.0"
  },
  "devDependencies": {
    "commitizen": "^4.2.1",
    "env-cmd": "^10.1.0",
    "fomantic-ui": "^2.8.4",
    "git-cz": "^4.7.1",
    "gulp": "^4.0.2",
    "react-snapshot": "^1.3.0"
  },
  "optionalDependencies": {
    "reselect": "^3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
